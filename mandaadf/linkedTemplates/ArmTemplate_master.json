{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "mandaadf"
		},
		"ADLSGen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLSGen2'"
		},
		"ADLSTripData_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLSTripData'"
		},
		"BlobStorageProducts_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BlobStorageProducts'"
		},
		"TaxiTripDB_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'TaxiTripDB'"
		},
		"TripAggregateBlob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'TripAggregateBlob'"
		},
		"mandasqlDb_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'mandasqlDb'"
		},
		"ADLSGen_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLSGen'"
		},
		"ADLSGen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mandadatalake.dfs.core.windows.net/"
		},
		"ADLSTripData_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mandadatalake.dfs.core.windows.net/"
		},
		"TaxiTripDB_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "mandaserver.database.windows.net"
		},
		"TaxiTripDB_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "taxidata"
		},
		"TaxiTripDB_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "manda"
		},
		"mandasqlDb_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "mandaserver.database.windows.net"
		},
		"mandasqlDb_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "mandasql"
		},
		"mandasqlDb_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "manda"
		},
		"ADLSGen_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mandadatalake.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "mandaadf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLSGen2_accountKey": {
						"value": "[parameters('ADLSGen2_accountKey')]"
					},
					"ADLSTripData_accountKey": {
						"value": "[parameters('ADLSTripData_accountKey')]"
					},
					"BlobStorageProducts_connectionString": {
						"value": "[parameters('BlobStorageProducts_connectionString')]"
					},
					"TaxiTripDB_password": {
						"value": "[parameters('TaxiTripDB_password')]"
					},
					"TripAggregateBlob_connectionString": {
						"value": "[parameters('TripAggregateBlob_connectionString')]"
					},
					"mandasqlDb_password": {
						"value": "[parameters('mandasqlDb_password')]"
					},
					"ADLSGen_accountKey": {
						"value": "[parameters('ADLSGen_accountKey')]"
					},
					"ADLSGen2_properties_typeProperties_url": {
						"value": "[parameters('ADLSGen2_properties_typeProperties_url')]"
					},
					"ADLSTripData_properties_typeProperties_url": {
						"value": "[parameters('ADLSTripData_properties_typeProperties_url')]"
					},
					"TaxiTripDB_properties_typeProperties_server": {
						"value": "[parameters('TaxiTripDB_properties_typeProperties_server')]"
					},
					"TaxiTripDB_properties_typeProperties_database": {
						"value": "[parameters('TaxiTripDB_properties_typeProperties_database')]"
					},
					"TaxiTripDB_properties_typeProperties_userName": {
						"value": "[parameters('TaxiTripDB_properties_typeProperties_userName')]"
					},
					"mandasqlDb_properties_typeProperties_server": {
						"value": "[parameters('mandasqlDb_properties_typeProperties_server')]"
					},
					"mandasqlDb_properties_typeProperties_database": {
						"value": "[parameters('mandasqlDb_properties_typeProperties_database')]"
					},
					"mandasqlDb_properties_typeProperties_userName": {
						"value": "[parameters('mandasqlDb_properties_typeProperties_userName')]"
					},
					"ADLSGen_properties_typeProperties_url": {
						"value": "[parameters('ADLSGen_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "mandaadf_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'mandaadf_ArmTemplate_0')]"
			]
		}
	]
}